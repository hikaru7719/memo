{
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.toolsEnvVars": {
    "GO111MODULE": "on"
  },
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "eslint.options": {
    "configfile": ".eslintrc.json"
  }
}
